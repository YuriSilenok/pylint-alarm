{
    "name": "pylint-buzzkill",
    "displayName": "Pylint Buzzkill",
    "description": "Plays a sound when pylint detects errors.",
    "version": "1.1.62",
    "engines": {
      "vscode": "^1.80.0"
    },
    "activationEvents": [
      "onLanguage:python"
    ],
    "main": "./extension.js",
    "contributes": {
      "commands": [
        {
          "command": "pylint-Buzzkill.enable",
          "title": "Enable Pylint Buzzkill"
        }
      ]
    },
    "devDependencies": {
      "child_process": "^1.0.2",
      "path": "^0.12.7",
      "vscode": "^1.1.37",
      "play-sound": "^1.1.3"
    },
    "extensionDependencies": [
      "ms-python.python"
    ]
  }
  
